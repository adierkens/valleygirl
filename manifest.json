{
  "manifest_version": 2,

  "name": "Like Blocker",
  "description": "Remove like buttons from your webpages.",
  "version": "1.0",

  "browser_action": {
    "default_title": "Like Blocker",
    "default_icon": "icon.png"
  },
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "js": ["jquery.js", "script.js"],
      "all_frames": true
    }
  ],
  "permissions": [
    "activeTab"
  ]
}
