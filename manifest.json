{
  "manifest_version": 2,

  "name": "Like Blocker",
  "description": "Remove like buttons from your webpages.",
  "version": "1.3.3.7",
  "icons": { "16": "icon16.png",
                   "48": "icon48.png",
                   "128": "icon128.png" },
  "browser_action": {
    "default_title": " "
  },
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "js": ["jquery.js", "script.js"],
      "all_frames": true
    }
  ],
  "permissions": [
    "activeTab"
  ]
}
